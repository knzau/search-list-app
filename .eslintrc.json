{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended", "plugin:react/jsx-runtime", "react-app"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react", "react-hooks"],
	"rules": { "react/prop-types": 0, "react-hooks/rules-of-hooks": "error", "react-hooks/exhaustive-deps": "warn" }
}
